@using Energy.Service;
@using Radzen.Blazor

@page "/overview"

<h3>Overview Total Power</h3>

@if (data is not null)
{
    <RadzenDataGrid PageSize="20" AllowMultiColumnSorting="true" ShowMultiColumnSortingIndex="true" AllowPaging="true" AllowSorting="true" Data="@data" TItem="EnergyData2" ColumnWidth="400px">
        <Columns>
            <RadzenDataGridColumn TItem="EnergyData2" Property="timeStamp" Title="Timestamp" Width="70px" />
            <RadzenDataGridColumn TItem="EnergyData2" Property="renewableShare" Title="Data" Width="120px" />
        </Columns>
    </RadzenDataGrid>
}